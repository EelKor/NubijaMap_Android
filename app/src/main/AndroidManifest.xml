<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.underbar.nubijaapp">
    <!-- 네이버 지도 위치 정보 수집 동의 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- 진동 권한 -->
    <uses-permission android:name="android.permission.VIBRATE" /> <!-- 인터넷 접속 권한 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="창원자전고"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NubijaApp"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity android:name=".MenuLicenseActivity"></activity>
        <activity android:name=".MenuAboutActivity" />
        <activity android:name=".MenuActivity" /> <!-- 액티비티 리스트 -->
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" /> <!-- 네이버 지도 인증 키 -->
        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="61eyickbg8" />
    </application>

</manifest>